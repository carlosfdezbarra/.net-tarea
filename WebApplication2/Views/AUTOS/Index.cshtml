@model IEnumerable<WebApplication2.Models.AUTOS>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Autos</h2>

<p>
    @Html.ActionLink("Agregar auto", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Patente
        </th>
        <th>
            Año
        </th>
        <th>
            Color
        </th>
        <th>
            Observación
        </th>
        <th>
            Modelo
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PATENTE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ANO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.COLOR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OBSERVACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MODELO.DESCRIPCION_MODELO)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_AUTO }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.ID_AUTO }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.ID_AUTO })
        </td>
    </tr>
}

</table>
